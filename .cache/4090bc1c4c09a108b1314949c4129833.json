":"components/Today.vue","original":{"line":52,"column":5}}],"sources":{"components/Today.vue":"<template>\n  <div class=\"com-today\">\n    <div class=\"week\">{{weekDay}}</div>\n    <div class=\"date\">{{date}}</div>\n  </div>\n</template>\n\n<script>\nimport moment from 'moment'\nconst WEEK_DAY = {\n  'Monday': '星期一',\n  'Tuesday': '星期二',\n  'Wednesday': '星期三',\n  'Thursday': '星期四',\n  'Friday': '星期五',\n  'Saturday': '星期六',\n  'Sunday': '星期天'\n}\nexport default {\n  name: 'today',\n  data () {\n    return {\n      weekDay: WEEK_DAY[moment().format('dddd')],\n      date: moment().format('D')\n    }\n  },\n  methods: {\n    showCalendar(){\n\n    }\n  }\n}\n</script>\n\n<style lang=\"less\">\n  .com-today{\n    display: inline-block;\n    width: 60px;\n    padding-top: 5px;\n    text-align: center;\n    background: #ff9966;\n    color: #fff;\n    border-radius: 4px;\n\n    .week{\n      font-size: 12px;\n    }\n    .date{\n      font-size: 30px;\n      font-weight: bolder;\n    }\n  }\n</style>"},"lineCount":null}},"hash":"81133084ef1b8c89a40c4afe828beb92","cacheData":{"env":{}}}